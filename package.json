{
  "name": "react-native-voice-stream",
  "version": "0.0.1",
  "description": "A React Native module for voice streaming as base-64 string chunks",
  "source": "src/index.ts",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "types": "lib/typescript/index.d.ts",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "react-native-voice-stream.podspec",
    "!lib/typescript/example",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "typecheck": "tsc",
    "clean": "del-cli android/build lib",
    "prepare": "bob build",
    "release": "release-it"
  },
  "keywords": [
    "react-native",
    "voice",
    "stream",
    "base-64",
    "native-module"
  ],
  "repository": "https://github.com/omalperera/react-native-voice-stream",
  "author": "Omal Perera <omalperera8@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/omalperera/react-native-voice-stream/issues"
  },
  "homepage": "https://github.com/omalperera/react-native-voice-stream#readme",
  "devDependencies": {
    "@react-native-community/bob": "^0.17.1",
    "@types/react": "^19.1.6",
    "del-cli": "^6.0.0",
    "react": "18.2.0",
    "react-native": "0.77.2",
    "release-it": "^15.6.0",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "@react-native-community/bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  }
}
